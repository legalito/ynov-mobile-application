<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.projet_dev_mobile.ui.home.HomeFragment"
        android:label="@string/title_menu_bike"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_park"
        android:name="com.example.projet_dev_mobile.ui.park.ParkFragment"
        android:label="@string/title_menu_park"
        tools:layout="@layout/fragment_park" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.projet_dev_mobile.ui.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
